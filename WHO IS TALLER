CODE:
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	for(int i=0; i<t; i++){
	    int x,y;
	    cin>>x>>y;
	    if(x>y)
	    cout<<"A"<<endl;
	    else
	    cout<<"B"<<endl;
	}
	return 0;
}
